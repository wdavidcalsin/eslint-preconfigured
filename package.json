{
  "name": "eslint-preconfigured",
  "private": false,
  "version": "2.9.9",
  "description": "This package provides a script that installs all the necessary dependencies for Eslint with Prettier and creates the Eslint configuration files.",
  "bin": "lib/bin.js",
  "main": "lib/index.js",
  "files": [
    "lib",
    "settings.sh",
    "settings.cmd"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/bin.ts --dts --out-dir lib",
    "eslint-preconfigured": "./settings.cmd",
    "prepare": "husky install"
  },
  "author": "@wdavid",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier",
    "prettier-config",
    "predefined-eslint",
    "eslint-preconfigured"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wdavidcalsin/eslint-preconfigured"
  },
  "homepage": "https://github.com/wdavidcalsin/eslint-preconfigured",
  "bugs": {
    "url": "https://github.com/wdavidcalsin/eslint-preconfigured/issues"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.5",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.2",
    "child_process": "^1.0.2",
    "depcheck": "^1.4.3",
    "fs": "0.0.1-security",
    "husky": "^8.0.0",
    "path": "^0.12.7",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "figlet": "^1.5.2",
    "fs-extra": "^11.1.0"
  }
}
